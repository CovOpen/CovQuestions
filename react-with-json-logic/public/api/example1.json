{
  "id": "aa43ae11-2b3c-46dd-ac01-c901c3d36a3e",
  "schemaVersion": "0.0.0.1",
  "version": "0.0.0.1",
  "meta": {
    "author": "Alexander",
    "language": "DE",
    "title": "Example",
    "creationDate": "2020-04-10T18:48:48+0000"
  },
  "questions": [
    {
      "id": "q1_contact",
      "text": "Gab es Kontakt zu bestätigten Fällen?",
      "type": "boolean"
    },
    {
      "id": "q2_contact_when",
      "text": "Wann trat der Kontakt auf?",
      "type": "date",
      "enableWhen": {
        "var": "q1_contact.value"
      }
    },
    {
      "id": "q3_symptoms",
      "text": "Symptome?",
      "type": "multiselect",
      "options": [
        {
          "text": "Husten",
          "value": "cough"
        },
        {
          "text": "Fieber",
          "value": "fever"
        },
        {
          "text": "Atembeschwerden",
          "value": "respiratory"
        }
      ]
    },
    {
      "id": "q4_symptoms_when",
      "text": "Ab wann gab es Symptome?",
      "type": "date",
      "enableWhen": {
        "var": "q3_symptoms.value"
      }
    },
    {
      "id": "q5_risk_factors",
      "text": "Gibt es Risikofaktoren?",
      "type": "boolean"
    },
    {
      "id": "q6_chronic_afflictions",
      "text": "Gibt es Chronische Beschwerden?",
      "type": "boolean"
    },
    {
      "id": "q6_medical_staff",
      "text": "Medizinisches Personal?",
      "type": "boolean",
      "enableWhen": {
        "in": [
          {
            "var": "v1_risk.value"
          },
          ["HIGH_RISK", "MEDIUM_RISK_A", "MEDIUM_RISK_B"]
        ]
      }
    }
  ],
  "variables": [
    {
      "id": "v_contact_relevant",
      "value": {
        "<=": [
          {
            "-": [
              {
                "var": "g_now.value"
              },
              {
                "var": "q2_contact_when.value"
              }
            ]
          },
          1209600
        ]
      }
    },
    {
      "id": "v_symptoms_relevant",
      "value": {
        ">=": [
          {
            "var": "q2_symptoms.count"
          },
          1
        ]
      }
    },
    {
      "id": "v1_risk",
      "value": {
        "if": [
          {
            "var": "q0_contact.value"
          },
          {
            "if": [
              {
                "var": "v_symptoms_relevant"
              },
              {
                "if": [
                  {
                    "<=": [
                      {
                        "-": [
                          {
                            "var": "q2_contact_when.value"
                          },
                          {
                            "var": "q4_symptoms_when.value"
                          }
                        ]
                      },
                      1209600
                    ]
                  },
                  "HIGH_RISK",
                  {
                    "if": [
                      {
                        "and": [
                          {
                            "some": [
                              {
                                "var": "q2_symptoms.value"
                              },
                              {
                                "==": [
                                  {
                                    "var": ""
                                  },
                                  "respiratory"
                                ]
                              }
                            ]
                          },
                          {
                            "some": [
                              {
                                "var": "q2_symptoms.value"
                              },
                              {
                                "==": [
                                  {
                                    "var": ""
                                  },
                                  "general"
                                ]
                              }
                            ]
                          },
                          {
                            "or": [
                              {
                                "var": "q5_risk_factors.value"
                              },
                              {
                                "var": "q6_chronic_afflictions.value"
                              }
                            ]
                          }
                        ]
                      },
                      "MEDIUM_RISK_A",
                      "MISSING"
                    ]
                  }
                ]
              },
              {
                "if": [
                  {
                    "var": "v_contact_relevant"
                  },
                  "MEDIUM_RISK_B",
                  "NO_RISK"
                ]
              }
            ]
          },
          {
            "if": [
              {
                "var": "v_symptoms_relevant"
              },
              {
                "if": [
                  {
                    "in": [
                      {
                        "var": "q2_symptoms.value"
                      },
                      ["respiratory"]
                    ]
                  },
                  "MEDIUM_RISK_B",
                  "NO_RISK"
                ]
              },
              "MISSING"
            ]
          }
        ]
      }
    }
  ],
  "resultCategories": [
    {
      "id": "rc_risk",
      "description": "Risikoeinschätzung",
      "results": [
        {
          "id": "MEDIUM_RISK_A",
          "text": "Mittleres Risiko",
          "value": {
            "==": [
              {
                "var": "v0_risk"
              },
              "MEDIUM_RISK_A"
            ]
          }
        },
        {
          "id": "MEDIUM_RISK_B",
          "text": "Mittleres Risiko",
          "value": {
            "==": [
              {
                "var": "v0_risk"
              },
              "MEDIUM_RISK_B"
            ]
          }
        },
        {
          "id": "HIGH_RISK",
          "text": "Hohes Risiko",
          "value": {
            "==": [
              {
                "var": "v0_risk"
              },
              "HIGH_RISK"
            ]
          }
        }
      ]
    },
    {
      "id": "rc_medical_advisory",
      "description": "Medizinischer Leitfaden",
      "results": [
        {
          "id": "SHOW_MEDICAL_ADVISORY",
          "text": "Hilfreiche Information.",
          "value": {
            "var": "q6_medical_staff.value"
          }
        }
      ]
    },
    {
      "id": "rc_contact_irrelevant",
      "description": "Kontakt",
      "results": [
        {
          "id": "SHOW_CONTACT_IRRELEVANT_ADVISORY",
          "text": "Der Kontakt war irrellevant.",
          "value": {
            "and": [
              {
                "var": "q1_contact.value"
              },
              {
                "!": {
                  "var": "v_contact_relevant.value"
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
