{
  "meta": {
    "author": "Alexander",
    "languages": [
      "DE"
    ],
    "name": "Example",
    "version": "0.1"
  },
  "questions": [
    {
      "id": "q1_contact",
      "text": "Gab es Kontakt zu bestätigten Fällen?",
      "type": "boolean"
    },
    {
      "id": "q2_contact_when",
      "text": "Wann trat der Kontakt auf?",
      "type": "date",
      "enableWhen": {
        "var": "q1_contact.value"
      }
    },
    {
      "id": "q3_symptoms",
      "text": "Symptome?",
      "type": "multiselect",
      "options": [
        {
          "text": "Husten",
          "value": "cough"
        },
        {
          "text": "Fieber",
          "value": "fever"
        },
        {
          "text": "Atembeschwerden",
          "value": "respiratory"
        }
      ]
    },
    {
      "id": "q4_symptoms_when",
      "text": "Ab wann gab es Symptome?",
      "type": "date",
      "enableWhen": {
        "var": "q3_symptoms.value"
      }
    },
    {
      "id": "q5_risk_factors",
      "text": "Gibt es Risikofaktoren?",
      "type": "boolean"
    },
    {
      "id": "q6_chronic_afflictions",
      "text": "Gibt es Chronische Beschwerden?",
      "type": "boolean"
    },
    {
      "id": "q6_medical_staff",
      "text": "Medizinisches Personal?",
      "type": "boolean",
      "enableWhen": {
        "in": [
          {
            "var": "v1_risk.value"
          },
          [
            "HIGH_RISK",
            "MEDIUM_RISK_A",
            "MEDIUM_RISK_B"
          ]
        ]
      }
    }
  ],
  "variables": [
    {
      "id": "v_contact_relevant",
      "value": {
        "<=": [
          {
            "-": [
              {
                "var": "q2_contact_when.value"
              },
              {
                "var": "g_now.value"
              }
            ]
          },
          14
        ]
      }
    },
    {
      "id": "v_symptoms_relevant",
      "value": {
        ">=": [
          {
            "var": "q2_symptoms.count"
          },
          1
        ]
      }
    },
    {
      "id": "v1_risk",
      "value": {
        "if": [
          {
            "var": "q0_contact.value"
          },
          {
            "if": [
              {
                "var": "v_symptoms_relevant"
              },
              {
                "if": [
                  {
                    "<=": [
                      {
                        "-": [
                          {
                            "var": "q2_contact_when.value"
                          },
                          {
                            "var": "q4_symptoms_when.value"
                          }
                        ]
                      },
                      14
                    ]
                  },
                  "HIGH_RISK",
                  {
                    "if": [
                      {
                        "and": [
                          {
                            "some": [
                              {
                                "var": "q2_symptoms.value"
                              },
                              {
                                "==": [
                                  {
                                    "var": ""
                                  },
                                  "respiratory"
                                ]
                              }
                            ]
                          },
                          {
                            "some": [
                              {
                                "var": "q2_symptoms.value"
                              },
                              {
                                "==": [
                                  {
                                    "var": ""
                                  },
                                  "general"
                                ]
                              }
                            ]
                          },
                          {
                            "or": [
                              {
                                "var": "q5_risk_factors.value"
                              },
                              {
                                "var": "q6_chronic_afflictions.value"
                              }
                            ]
                          }
                        ]
                      },
                      "MEDIUM_RISK_A",
                      "MISSING"
                    ]
                  }
                ]
              },
              {
                "if": [
                  {
                    "var": "v_contact_relevant"
                  },
                  "MEDIUM_RISK_B",
                  "NO_RISK"
                ]
              }
            ]
          },
          {
            "if": [
              {
                "var": "v_symptoms_relevant"
              },
              {
                "if": [
                  {
                    "in": [
                      {
                        "var": "q2_symptoms.value"
                      },
                      [
                        "respiratory"
                      ]
                    ]
                  },
                  "MEDIUM_RISK_B",
                  "NO_RISK"
                ]
              },
              "MISSING"
            ]
          }
        ]
      }
    },
    {
      "id": "v2_needs_medical_advisory",
      "value": {
        "var": "q6_medical_staff.value"
      }
    },
    {
      "id": "v3_contact_irrelevant_notice",
      "value": {
        "and": [
          {
            "var": "q0_contact.value"
          },
          {
            "!": {
              "var": "v_contact_relevant"
            }
          }
        ]
      }
    }
  ],
  "resultCategories": [
    {
      "id": "rc_risk",
      "text": "Risikoeinschätzung",
      "results": [
        {
          "id": "MEDIUM_RISK_A",
          "text": "Mittleres Risiko"
        },
        {
          "id": "MEDIUM_RISK_B",
          "text": "Mittleres Risiko"
        },
        {
          "id": "HIGH_RISK",
          "text": "Hohes Risiko"
        }
      ],
      "value": {
        "var": "v0_risk"
      }
    },
    {
      "id": "rc_medical_advisory",
      "text": "Medizinischer Leitfaden",
      "results": [
        {
          "id": "SHOW_MEDICAL_ADVISORY",
          "text": "Hilfreiche Information."
        }
      ],
      "value": {
        "if": [
          {
            "var": "v2_needs_medical_advisory"
          },
          "SHOW_MEDICAL_ADVISORY",
          "NONE"
        ]
      }
    },
    {
      "id": "rc_contact_irrelevant",
      "text": "Kontakt",
      "results": [
        {
          "id": "SHOW_CONTACT_ADVISORY",
          "text": "Der Kontakt wahr irrellevant."
        }
      ],
      "value": {
        "if": [
          {
            "var": "v3_contact_irrelevant_notice"
          },
          "SHOW_CONTACT_ADVISORY",
          "NONE"
        ]
      }
    }
  ]
}