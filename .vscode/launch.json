  "version": "0.2.0",
  "configurations": [
    {
      "name": "Index TS File",
      "type": "node",
      "request": "launch",
      "args": ["src/index.ts"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/api/v1/",
      "protocol": "inspector",
      ""
    },
    {
      "name": "I18n TS File",
      "type": "node",
      "request": "launch",
      "args": ["scripts/i18n_extract.ts"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}/api/v1/",
      "protocol": "inspector",
      ""
    }
  ]
}
