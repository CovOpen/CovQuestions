{
  "name": "cov-question-api-v1",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "pre-build": "npm run pre-build-add && npm run pre-build-trans",
    "pre-build-add": "ts-node scripts/pre_build_add_new_file.ts",
    "pre-build-trans": "ts-node scripts/pre_build_fixate_translations.ts",
    "build": "rimraf dist && ts-node ./index.ts && npm run build-swagger",
    "build-swagger": "copyfiles -f ./src/swagger/index.html ./dist/swagger/ && copyfiles ./openapi.json ./dist/swagger/",
    "test": "jest",
    "installAndLink": "(cd ../../covquestions-js && npm install && npm run build) && npm install && npm link ../../covquestions-js/dist"
  },
  "author": "",
  "dependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/node": "^13.11.1",
    "ajv": "^6.12.0",
    "copyfiles": "^2.2.0",
    "fast-glob": "^3.2.2",
    "fs-extra": "^9.0.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "xmlbuilder2": "^2.1.2"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.4.0",
    "ts-jest": "^25.4.0"
  }
}
